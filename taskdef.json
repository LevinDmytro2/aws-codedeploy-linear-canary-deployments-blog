{
    "executionRoleArn": "arn:aws:iam::360119372877:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "dmytroapp2",
            "image": "360119372877.dkr.ecr.us-east-1.amazonaws.com/ecs-sample-app:latest",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "ecs-demo"
}